<%= render partial: '/layouts/errors', locals: {object: @experience} %>

<%= form_for @experience do |f| %>

    <%= f.label :rating, "Rating 1-10" %>
    <%= f.number_field :rating, min: 1, max: 10 %><br>

    <%= f.label :notes %><br>
    <%= f.text_area :notes %><br>

    <%= f.hidden_field :medium_id %>

    <%= f.submit "Record Experience" %>

<% end %>